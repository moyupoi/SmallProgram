page {
  background: #F2F2F2;
}
.home {
  width: 100%;
  height: 100%;
}
.banner {
  height: 240rpx;
  text-align: center;
  background: #00DBB2;
  color: #fff;
  overflow: hidden;
}
.banner .portrait {
  display: block;
  width: 100rpx;
  height: 100rpx;
  margin: 20rpx auto 0 auto;
  clip-path: circle(50% at center);
  overflow: hidden;
}
.banner .userName {
  margin-top: 20rpx;
  display: block;
}
.content {
  font-weight: 600;
  padding: 0 40rpx;
}
.content .describe {
  font-size: 30rpx;
  font-weight: 500;
  height: 120rpx;
  line-height: 50rpx;
  text-align: right;
  /* display: block; */
  background: #fff;
  text-align: center;
  color: #666666;
  border-radius: 20rpx;
  padding-top: 22rpx;
  margin-top: 40rpx;
}
.content .testing, .content .group, .content .groupPK {
  background: #00DBB2;
  color: #fff;
  font-weight: 400;
  width: 500rpx;
  border-radius: 50rpx;
  border: 0;
  margin-top: 40rpx;
}
.content .toBankList {
  width: 100%;
  height: 160rpx;
  margin-top: 80rpx;
  border: 2rpx solid #010101;
  background: #fff;
  padding: 20rpx 0;
  position: relative;
  border-radius: 20rpx;
  margin-bottom: 100rpx;
}
.content .toBankList::before {
  content: '';
  width: 68rpx;
  height: 70rpx;
  position: absolute;
  top: 40rpx;
  right: 30rpx;
  background: url('data:image/jpeg;base64,') no-repeat;
  background-size: 68rpx 70rpx;
}
.content .toBankList text {
  display: block;
  font-size: 36rpx;
  line-height: 60rpx;
  color: #010101;
}
.content .toBankList text:nth-of-type(2) {
  display: block;
  font-size: 26rpx;
}
.rankings {
  height: 600rpx;
  background: #fff;
  margin: 0 auto;
  border-radius: 20rpx;
  margin-top: 20rpx;
}
.rankings .slide {
  padding: 0 20rpx;
  overflow: auto;
  height: 500rpx;
}
.rankings .rankingList {

}
.rankings .title {
  display: block;
  font-size: 32rpx;
  line-height: 80rpx;
  color: #494949;
  text-align: center;
}
.rankings .rankingList .rankingItem {
  margin-bottom: 15rpx;
}
.rankings .rankingList .rankingItem .rankingImg {
  width: 80rpx;
  height: 80rpx;
  display: inline-block;
  margin-right: 15rpx;
}
.rankings .rankingList .rankingItem .portrait {
  width: 80rpx;
  height: 80rpx;
  display: inline-block;
  margin-right: 15rpx;
  border-radius: 50rpx;
}
.rankings .rankingList .rankingItem view {
  display: inline-block;
  vertical-align: top;
}
.rankings .rankingList .rankingItem view text {
  font-size: 28rpx;
  display: block;
  color: #7D7D7D;
  font-weight: 400;
}
.comparison {
  background: #fff;
  margin-top: 20rpx;
  border-radius: 20rpx;
  padding: 20rpx 20rpx;
}
.comparison .userGroup {

}
.comparison .userGroup image {
  width: 90rpx;
  height: 90rpx;
  display: inline-block;
  border-radius: 50rpx;
  margin-right: 20rpx;
}
.comparison .userGroup view {
  display: inline-block;
  vertical-align: super;
}
.comparison .userGroup view text {
  font-size: 26rpx;
  display: block;
  color: #494949;
}
.comparison .userGroup view text:nth-of-type(1) {
  margin-bottom: 10rpx;
}
