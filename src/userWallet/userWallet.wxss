page {
  background: #F2F2F2;
}
button {
  border: 0;
  padding: 0;
}
.walletFrame {
  width: 100%;
  height: 100%;
}
.deposit {
  background: #fff;
  height: .deposit ;
  overflow: hidden;
  background: #31DCB2;
  overflow: hidden;
  text-align: center;
  padding-bottom: 72rpx;
  margin-bottom: 16rpx;
}
.deposit .title {
  display: block;
  text-align: center;
  font-size: 28rpx;
  color: #fff;
  margin-top: 22rpx;
  margin-bottom: 20rpx;
}
.deposit .symbol {
  display: inline-block;
  text-align: center;
  font-size: 48rpx;
  color: #fff;
}
.deposit .totalAmount {
  display: inline-block;
  text-align: center;
  font-size: 84rpx;
  color: #fff;
}
.deposit .frequency {
  display: table;
  margin: 0 auto;
  font-size: 28rpx;
  font-weight: 400;
  color: #fff;
  background: #00C18D;
  padding: 4rpx 25rpx;
  border-radius: 50rpx;
}
.cashForm {
  width: 686rpx;
  background: #fff;
  padding: 0 32rpx;
}
.cashForm .money {
  position: relative;
  border-bottom: 1rpx solid #EAEAEA;
  overflow: hidden;
  padding: 32rpx 0;
}
.cashForm .money text {
  width: 170rpx;
  font-size: 32rpx;
  color: #999999;
  display: block;
  float: left;
  margin-top: 5rpx;
}
.cashForm .money input {
  width: 260rpx;
  display: block;
  float: left;
  margin-left: 20rpx;
}
.cashForm .money label {
  width: 32rpx;
  height: 32rpx;
  position: absolute;
  top: 44rpx;
  right: 184rpx;
  background: url('data:image/png;base64,') no-repeat;
  background-size: 32rpx 32rpx;
}
.cashForm .money button {
  width: 112rpx;
  height: 50rpx;
  line-height: 50rpx;
  background: #fff;
  color: #01DBB1;
  font-size: 28rpx;
  position: absolute;
  top: 38rpx;
  right: 20rpx;
}
.cashForm .code {
  position: relative;
  overflow: hidden;
  padding: 32rpx 0;
}
.cashForm .code text {
  width: 170rpx;
  font-size: 32rpx;
  color: #999999;
  display: block;
  float: left;
  margin-top: 5rpx;
}
.cashForm .code input {
  width: 260rpx;
  display: block;
  float: left;
  margin-left: 20rpx;
}
.cashForm .code image {
  display: block;
  width: 158rpx;
  height: 58rpx;
  background: #fff;
  position: absolute;
  top: 23rpx;
  right: 0;
  z-index: 2;
}
.withdraw {
  width: 686rpx;
  height: 96rpx;
  display: block;
  text-align: center;
  color: #fff;
  background: #01DBB1;
  margin-top: 40rpx;
}
.shortMessageCode {
  text-align: center;
  margin-top: 120rpx;
  overflow: hidden;
}
.shortMessageCode text {
  font-size: 36rpx;
  font-weight: 600;
}
.shortMessageCode .labMassageCode {
  height: 400rpx;
  position: relative;
  z-index: 10;
}
.shortMessageCode .changeNumber {
  position: relative;
  width: 60rpx;
  margin-right: 32rpx;
  display: inline-block;
}
.shortMessageCode .changeNumber label {
  position: absolute;
  top: 55rpx;
  left: 25rpx;
  width: 60rpx;
  height: 4rpx;
  background: #D8D8D8;
}
.shortMessageCode .changeNumber text {
  position: absolute;
  top: -8rpx;
  left: 33rpx;
  font-size: 80rpx;
  font-weight: 600;
}
.shortMessageCode input {
  width: 0;
  height: 0;
  position: absolute;
  z-index: 0;
  outline: none;
  border: 0;
}
.success {
  overflow: hidden;
  padding: 0 32rpx;
}
.success image {
  display: block;
  text-align: center;
  width: 280rpx;
  height: 206rpx;
  margin: 160rpx auto 60rpx auto;
}
.success text {
  font-size: 36rpx;
  color: #333333;
  font-weight: 600;
  text-align: center;
  display: block;
}
.success button {
  margin-top: 140rpx;
  height: 96rpx;
  background: #01DBB1;
  color: #fff;
}
