page {
  background: #F2F2F2;
}
.personalCenter {
  width: 100%;
  height: 100%;
}
.banner {
  height: 240rpx;
  text-align: center;
  background: #00DBB2;
  color: #fff;
  overflow: hidden;
}
.banner .portrait {
  display: block;
  width: 100rpx;
  height: 100rpx;
  margin: 20rpx auto 0 auto;
  clip-path: circle(50% at center);
  overflow: hidden;
}
.banner .userName {
  margin-top: 20rpx;
  display: block;
}
.queryProject {
  background: #FFF;
  margin-top: 20rpx;
}
.queryProject navigator {
  height: 150rpx;
  border-bottom: 1rpx solid #F2F2F2;
}
.queryProject navigator image {
  width: 80rpx;
  height: 80rpx;
  float: left;
  margin: 38rpx 30rpx 0 50rpx;
}
.queryProject navigator text {
  font-size: 30rpx;
  line-height: 150rpx;
  float: left;
}
.feedback {
  background: #fff;
  height: 150rpx;
  margin-top: 20rpx;
}
.feedback image {
  width: 80rpx;
  height: 80rpx;
  float: left;
  margin: 38rpx 30rpx 0 50rpx;
}
.feedback text {
  font-size: 30rpx;
  line-height: 150rpx;
  float: left;
}
